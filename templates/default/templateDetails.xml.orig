<?xml version="1.0" encoding="utf-8"?>
<install version="1.5" type="template">
	<name>Default</name>
	<version>3.0_Alpha1.2</version>
	<creationDate>March 2011</creationDate>
	<author>JomSocial Team</author>
	<authorEmail>support@azrul.com</authorEmail>
	<authorUrl>http://www.jomsocial.com</authorUrl>
	<copyright>Copyright 2008 - 2011 by Slashes &amp; Dots Sdn Bhd. All rights reserved</copyright>
	<license>http://www.gnu.org/licenses/gpl-2.0.html GNU/GPL</license>
	<description>Default template for JomSocial</description>

	<params>
		<param type="spacer" default="Frontpage" />
		<param name="showsearch" type="list" default="1" label="Show search box" description="">
			<option value="1" name='Show'>Show</option>
			<option value="0" name='Hide'>Hide</option>
			<option value="2" name='Members'>Members Only</option>
		</param>
		<param name="showlatestgroups" type="list" default="1" label="Show latest group" description="">
			<option value="1" name='Show'>Show</option>
			<option value="0" name='Hide'>Hide</option>
			<option value="2" name='Members'>Members Only</option>
		</param>
		<param name="showlatestphotos" type="list" default="1" label="Show new photos" description="">
			<option value="1" name='Show'>Show</option>
			<option value="0" name='Hide'>Hide</option>
			<option value="2" name='Members'>Members Only</option>
		</param>
		<param name="showonline" type="list" default="1" label="Show who's online" description="">
			<option value="1" name='Show'>Show</option>
			<option value="0" name='Hide'>Hide</option>
			<option value="2" name='Members'>Members Only</option>
		</param>
		<param name="showlatestmembers" type="list" default="1" label="Show recent members" description="">
			<option value="1" name='Show'>Show</option>
			<option value="0" name='Hide'>Hide</option>
			<option value="2" name='Members'>Members Only</option>
		</param>
		<param name="showlatestvideos" type="list" default="1" label="Show recent videos" description="">
			<option value="1" name='Show'>Show</option>
			<option value="0" name='Hide'>Hide</option>
			<option value="2" name='Members'>Members Only</option>
		</param>
		<param name="frontpage_latest_events" type="list" default="1" label="Show latest events" description="">
			<option value="1" name='Show'>Show</option>
			<option value="0" name='Hide'>Hide</option>
			<option value="2" name='Members'>Members Only</option>
		</param>
		<param name="showmembercount" type="radio" default="1" label="Show members count" description="">
			<option value="1" name='Show'>Show</option>
			<option value="0" name='Hide'>Hide</option>
		</param>
		<param type="spacer" default="Applications" />
		<param name="appsShowAuthor" type="radio" default="1" label="Show plugin author" description="">
			<option value="1" name='Show'>Show</option>
			<option value="0" name='Hide'>Hide</option>
		</param>
		<param name="colorTheme" type="list" default="green" label="Color theme" description="">
			<option value="green" name="Green">Green</option>
			<option value="blue" name="Blue">Blue</option>
			<option value="pink" name="Pink">Pink</option>
			<option value="dark" name="Dark">Dark</option>
		</param>
	</params>
</install>
